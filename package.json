{
  "name": "guess-who",
  "description": "Guess Who browser game",
  "version": "0.0.0",
  "author": "Kriss Watt <kriss.watt@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c"
  },
  "eslintConfig": {
    "extends": "arenanet",
    "env": {
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "one-var": [
        2,
        {
          "const": "never"
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^4.4.0",
    "eslint-config-arenanet": "^3.3.1",
    "modular-css": "^6.0.2",
    "rollup": "^0.45.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "mithril": "^1.1.3"
  }
}
